{
  "name": "cardo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:watch": "vitest --watch",
    "lint": "biome check src",
    "lint:fix": "biome check --fix src && prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "cardo": "./dist/index.js"
  },
  "oclif": {
    "bin": "cardo",
    "dirname": "cardo",
    "commands": "./src/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=24.0.0"
  },
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "@biomejs/biome": "^2.2.2",
    "@oclif/core": "^4.5.2",
    "@oclif/plugin-help": "^6.2.32",
    "@oclif/plugin-plugins": "^5.4.46",
    "@prisma/client": "^6.15.0",
    "@types/node": "^24.3.0",
    "any-llm": "^0.1.26",
    "console-table-printer": "^2.14.6",
    "dotenv": "^17.2.1",
    "open": "^10.2.0",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "zod": "^4.1.5"
  }
}
